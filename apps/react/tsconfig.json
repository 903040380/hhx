{
  "compilerOptions": {
    "jsx": "react",
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": false,
    "module": "ES2022",
    "target": "es5",
    "allowJs": true,
    "moduleResolution":"Node",
    "baseUrl": ".",
    "outDir": "dist",
    "paths": {
      "@src/*":["src/*"]
    },
  },
  "include": [
    "src"
  ],
}
